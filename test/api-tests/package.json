{
  "name": "xspacegrow-api-tests",
  "version": "1.0.0",
  "description": "Comprehensive test suite for XSpaceGrow API including authentication, device management, subscription billing, suspension system, WebSocket communication, and load testing",
  "main": "index.js",
  "scripts": {
    "test": "node index.js all",
    "test:auth": "node test-auth.js",
    "test:devices": "node test-devices.js",
    "test:subscriptions": "node test-subscriptions.js",
    "test:suspension": "node test-suspension.js",
    "test:websocket": "node test-websocket.js",
    "test:load": "node test-load.js",
    "test:all": "npm run test:auth && npm run test:devices && npm run test:subscriptions && npm run test:suspension && npm run test:websocket && npm run test:load",
    "test:enhanced": "../run-all-tests-enhanced.sh",
    "test:status": "node index.js status",
    "test:list": "node index.js list",
    "setup": "npm install && echo 'Test environment ready!'",
    "clean": "rm -f *-test-results-*.json && echo 'Test results cleaned'",
    "report": "ls -la *-test-results-*.json 2>/dev/null || echo 'No test results found'",
    "export": "node -e \"const fs=require('fs'); const files=fs.readdirSync('.').filter(f=>f.includes('test-results')); console.log('Available reports:', files.join(', '));\"",
    "benchmark": "npm run test:load",
    "validate": "npm run test:status && npm run test:auth"
  },
  "keywords": [
    "api",
    "testing",
    "xspacegrow",
    "iot",
    "subscription",
    "billing",
    "suspension",
    "device-management",
    "websocket",
    "load-testing",
    "authentication"
  ],
  "author": "XSpaceGrow Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "json2csv": "^5.0.7",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "autocannon": "^7.15.0",
    "clinic": "^9.1.0",
    "eslint": "^8.50.0",
    "loadtest": "^8.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xspacegrow/api-tests.git"
  },
  "bugs": {
    "url": "https://github.com/xspacegrow/api-tests/issues"
  },
  "homepage": "https://github.com/xspacegrow/api-tests#readme"
}